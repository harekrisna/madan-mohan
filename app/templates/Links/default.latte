</div>
{/block}
{block scripts}
{include parent}
  <script type="text/javascript">
    $('div.link-box').click(function() {
      var href = $(this).children("a").attr("href");
      var win=window.open(href, '_blank');
      win.focus();
    });
        
    $('#links a').click(function() {
    	var win = window.open($(this).attr("href"), '_blank');
		win.focus();
      	return false;
    });
  </script>
{/block}
